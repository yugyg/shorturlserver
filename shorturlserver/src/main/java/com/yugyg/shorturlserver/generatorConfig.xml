			userId="sa" password="Wx123456">
		</jdbcConnection>



		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- java model类生成路径 -->
		<javaModelGenerator
			targetPackage="com.yugyg.shorturlserver.entity.automodel"
			targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- java sqlmap xml映射文件地址 -->
		<sqlMapGenerator targetPackage="autosqlmap"
			targetProject="./src/main/resources">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</sqlMapGenerator>

		<!-- java dao interface接口 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.yugyg.shorturlserver.entity.autodao"
			targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
